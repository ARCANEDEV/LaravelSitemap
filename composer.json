{
    "name": "arcanedev/laravel-sitemap",
    "description": "A sitemap generator for Laravel.",
    "homepage": "https://github.com/ARCANEDEV/LaravelSitemap",
    "keywords": ["arcanedev", "laravel", "sitemap", "generator"],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=7.2.0",
        "ext-dom":           "*",
        "ext-json":          "*",
        "arcanedev/support": "^5.1.1"
    },
    "require-dev": {
        "orchestra/testbench":                "^4.0",
        "phpunit/phpcov":                     "^6.0",
        "phpunit/phpunit":                    "^8.0",
        "spatie/phpunit-snapshot-assertions": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelSitemap\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelSitemap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\LaravelSitemap\\LaravelSitemapServiceProvider"
            ]
        }
    }
}
