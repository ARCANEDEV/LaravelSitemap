{
    "name": "arcanedev/laravel-sitemap",
    "description": "A sitemap generator for Laravel.",
    "homepage": "https://github.com/ARCANEDEV/LaravelSitemap",
    "keywords": ["arcanedev", "laravel", "sitemap", "generator"],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=5.5.9",
        "arcanedev/support": "~3.0"
    },
    "require-dev": {
        "phpunit/phpcov":    "~2.0|~3.0",
        "phpunit/phpunit":   "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelSitemap\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelSitemap\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
